{
  "name": "github-metrics",
  "type": "module",
  "version": "0.0.0",
  "description": "Discover comprehensive insights into your GitHub repositories' metrics. Track stars, forks, and more, all conveniently stored locally",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:ts": "vue-tsc --noEmit",
    "lint:js": "eslint .",
    "lint:js:fix": "pnpm run lint:js -- --fix",
    "lint:css": "stylelint **/*{.scss,.vue}",
    "lint:css:fix": "pnpm run lint:css -- --fix",
    "lint:all": "pnpm run lint:ts && pnpm run lint:js && pnpm run lint:css"
  },
  "dependencies": {
    "@octokit/core": "^6.1.2",
    "@tabler/icons-vue": "^3.12.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vueuse/core": "^10.11.1",
    "@vueuse/integrations": "^10.11.1",
    "dayjs": "^1.11.12",
    "http-status-codes": "^2.3.0",
    "semver": "^7.6.3",
    "sortablejs": "^1.15.2",
    "vite": "^5.4.0",
    "vite-plugin-pwa": "^0.20.1",
    "vue": "^3.4.37",
    "vue-dompurify-html": "^5.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.0",
